{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "max-len": ["error": {"code": 120}],
    "react/react-in-jsx-scope": "off",
    "object-curly-newline": "off",
    "react/prop-types": "off",
    "jsx-a11y/anchor-is-valid": "off"
  }
}
